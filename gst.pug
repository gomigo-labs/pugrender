body
    div(class="div-center")
        div(class="body-center height-100 width-100 monst bg-white position-relative")
            div(class="flex-container pn br-1")
              div(class="width-100 flex-container pn bright-1")
                  if invoiceJsonData.invoiceLogo && invoiceJsonData.invoiceLogo.length > 0
                      div(class="")
                          img(src=invoiceJsonData.invoiceLogo height="80px" width="80px ")
                  div(class=" width-100")
                      div(class="monst mt-5")
                          span(class="go") !{orgJson.businessName}
                      div(class="monst mt-5  width-100")
                          div(class="text-secondary width-100") !{orgJson.permanentAddress.address_line_1}, !{orgJson.permanentAddress.address_line_2 }
                      div(class="mt-5")
                          span(class="text-secondary") PHONE:
                          span(class="text-secondary label-Padding") !{orgJson.organizationAccount.mobileNo}
                          span(class="text-secondary") GSTIN:
                          span(class="text-secondary label-Padding") !{orgJson.GSTIN}

              div(class="width-100 flex-center-center ")
                div(class="width-100 flex-column-center")
                    div(class="width-100 text-secondary fw-500 text-align-center") Invoice Number
                    div(class=" width-100 text-secondary text-align-center") !{invoiceJsonData.invoiceNumber}
                div(class="width-100 flex-column-center")
                    div(class="width-100 text-secondary fw-500 text-align-center") Invoice Date
                    div(class="width-100 text-secondary text-align-center") !{invoiceDate}
                div(class="width-100 flex-column-center")
                    div(class="width-100 text-secondary fw-500 text-align-center") Due Date
                    div(class="width-100 text-secondary text-align-center") !{dueDate}

            div(class="flex-container pn br-1 bt-none")
              div(class="width-100 flex-container bright-1")
                div(class="width-100 ph-10 text-secondary")
                  div(class="bold") BILL TO
                  div(class="go text-secondary") !{invoiceJsonData.customerName}
                  if invoiceJsonData.customerAddress.billingAddress.addressLine1
                    div(class="text-secondary width-90-per") !{invoiceJsonData.customerAddress.billingAddress.addressLine1},
                  if invoiceJsonData.customerAddress.billingAddress.addressLine2
                    div(class="text-secondary width-90-per") !{invoiceJsonData.customerAddress.billingAddress.addressLine2},
                  div() !{invoiceJsonData.customerAddress.billingAddress.state}, !{invoiceJsonData.customerAddress.billingAddress.pinCode}

              div(class="width-100 flex-container")
                div(class="width-100 ph-10 text-secondary")
                  div(class="bold") SHIP TO
                  div(class="go text-secondary") !{invoiceJsonData.customerName}
                  if invoiceJsonData.customerAddress.shippingAddress.addressLine1
                    div(class="text-secondary width-90-per") !{invoiceJsonData.customerAddress.shippingAddress.addressLine1},
                  if invoiceJsonData.customerAddress.shippingAddress.addressLine2
                    div(class="text-secondary width-90-per") !{invoiceJsonData.customerAddress.shippingAddress.addressLine2},
                  div(class="text-secondary") !{invoiceJsonData.customerAddress.shippingAddress.state}, !{invoiceJsonData.customerAddress.shippingAddress.pinCode}
                  div(class="flex-container pn")
                    div(class="text-secondary pr-10 bold width-30") GSTIN:
                    div(class="text-secondary") !{invoiceJsonData.customerGSTIN}
            
            div(class="flex bg-grey br-1 bt-none")
                div(class="width-200 ")
                    div(class="bold fs-10") ITEMS
                div(class="width-100 ")
                    div(class="bold fs-10 ") HSN
                div(class="width-100 ")
                    div(class="bold fs-10 ") QTY
                div(class="width-100 ")
                    div(class="bold fs-10 ") RATE
                div(class="width-100 ")
                    div(class="bold fs-10 ") UOM
                div(class="width-100 ")
                    div(class="bold fs-10 ") DISC.(%)
                div(class="width-100 ")
                    div(class="bold fs-10 ") TAX
                div(class="width-100 ")
                    div(class="bold fs-10 ") AMT.

            div(class="table-height br-1 bt-none")
              each val in lineItems
                  div(class="flex bb-1")
                      div(class="flex-align width-200 pn pl-10")
                          div(class="fs-10") !{val.itemName}
                          div(class="fs-10 color-secondary") !{val.itemCode}
                      div(class="width-100 flex-align pn")
                          div(class="fs-10") !{val.itemHsnOrSac}
                          div(class="width-100 ")
                      div(class="width-100 flex-align pn")
                          div(class="fs-10") !{val.itemQty}
                          div(class="width-100 ")
                      div(class="width-100 flex-align pn")
                          div(class="fs-10") !{val.itemPrice}
                          div(class="width-100 ")
                      div(class="width-100 flex-align pn")
                          div(class="fs-10") !{val.uom}
                          div(class="width-100 ")
                      div(class="width-100 flex-align pn")
                          div(class="fs-10") !{val.discount}
                          div(class="width-100 ")
                      div(class="width-100 flex-align pn")
                          div(class="fs-10") !{Number(val.itemAmount - val.taxableAmount).toFixed(2)}
                          div(class="width-100 fs-10 color-secondary") (!{val.itemTax} %)
                      div(class="width-100 flex-align pn pr-10")
                          div(class="fs-10") !{val.itemAmount}
                          div(class="width-100 ")

            div(class="flex height-30 br-1 bt-none bg-grey")
                div(class="width-200 ")
                    div(class="bold fs-10") Total Amount
                div(class="width-100 ")
                    div(class="bold fs-10 ")
                div(class="width-100 ")
                    div(class="bold fs-10 ") !{lineItems.reduce((acc, obj) => acc + Number(obj.itemQty), 0)}
                div(class="width-100 ")
                    div(class="bold fs-10 ") 
                div(class="width-100 ")
                    div(class="bold fs-10 ") 
                div(class="width-100 ")
                    div(class="bold fs-10 ")
                div(class="width-100 ")
                    div(class="bold fs-10 ") ₹ !{lineItems.reduce((acc, obj) => acc + Number(obj.itemAmount - obj.taxableAmount), 0).toFixed(2)}
                div(class="width-100 ")
                    div(class="bold fs-10 ") ₹ !{lineItems.reduce((acc, obj) => acc + Number(obj.itemAmount), 0).toFixed(2)}

            div(class="flex height-30 br-1 bt-none")
                div(class="width-200 ")
                    div(class="bold fs-10") Receive Amount
                div(class="width-100 ")
                    div(class="bold fs-10 ")
                div(class="width-100 ")
                    div(class="bold fs-10 ") 
                div(class="width-100 ")
                    div(class="bold fs-10 ") 
                div(class="width-100 ")
                    div(class="bold fs-10 ") 
                div(class="width-100 ")
                    div(class="bold fs-10 ")
